{
  "name": "typegator",
  "version": "1.0.0",
  "description": "Um quase motor de buscar, um projeto para mostrar minhas quase habilidades",
  "main": "server.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joashneves/typegator/tree/main"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "concurrently \"cd front && npm run dev\" \"nodemon server.js\"",
    "test:watch": "jest --watchAll --runInBand --verbose",
    "start:api": "nodemon server.js",
    "start:front": "cd front && npm start",
    "start": "concurrently \"npm run start:api\" \"npm run start:front\"",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "prepare": "husky",
    "commit": "cz",
    "build": "cd front && npm install && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-cache-controller": "^1.1.0",
    "express-jwt": "^8.5.1",
    "express-openid-connect": "^2.17.1",
    "glob": "^11.0.1",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "rimraf": "^6.0.1",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7",
    "typegator": "file:"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "commitizen": "^4.3.1",
    "concurrently": "^9.1.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "sequelize-cli": "^6.6.2",
    "vite": "^6.0.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
